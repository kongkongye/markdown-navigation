{
	"name": "markdownnavigation",
	"displayName": "Markdown Navigation",
	"description": "Markdown Navigation",
	"version": "1.0.1",
	"publisher": "kongkongye",
	"icon": "resources/description.png",
	"engines": {
		"vscode": "^1.68.0"
	},
	"categories": [
		"Visualization",
		"Other"
	],
	"keywords": [
		"markdown",
		"navigation",
		"toc"
	],
	"activationEvents": [
		"onLanguage:markdown"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/kongkongye/MarkdownNavigation.git"
	},
	"license": "MIT",
	"main": "./dist/extension.js",
	"contributes": {
		"views": {
			"explorer": [
				{
					"id": "MarkdownNavigation",
					"name": "%markdown_navigation_view_name%",
					"when": "resourceLangId == 'markdown'"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.68.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.7.2",
		"ts-loader": "^9.3.0",
		"webpack": "^5.72.1",
		"webpack-cli": "^4.9.2",
		"@vscode/test-electron": "^2.1.3"
	}
}